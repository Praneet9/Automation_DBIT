<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.vinay.automation.RailwayReservation">

    <Button
        android:id="@+id/newUser"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="109dp"
        android:onClick="newUserClicked"
        android:text="New User" />

    <Button
        android:id="@+id/existingUser"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/newUser"
        android:layout_alignRight="@+id/newUser"
        android:layout_below="@+id/newUser"
        android:layout_marginTop="92dp"
        android:onClick="existingUserClicked"
        android:text="Existing User" />
</RelativeLayout>
